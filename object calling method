// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class smartphone{
    public:
    string modelName;
    int price;
    void phoneDetails(){
  cout<<"phone model:"<<modelName<<endl;
  cout<<"phone price:"<<price<<endl;
    }
 void call(){
     cout<<modelName<<" "<<"calling..."<<endl;
 }
  void disconnect(){
     cout<<modelName<<" "<<"disconnected";
 }
};
int main() {
 smartphone phone1,phone2;
 phone1.modelName="iphone 12";
 phone1.price=20000;
 phone2.modelName="samsung";
 phone2.price=10000;
 phone1.call();
phone2.disconnect();
    return 0;
}
