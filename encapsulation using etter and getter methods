// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class bankAC{
    private:
    int acBalance;
    public:
    void setBalance(int b){
        acBalance=b;
    }
  
    int getBalance(){
        return acBalance;
    }
};


int main() {
   bankAC ac;
   ac.setBalance(10000);
   cout<<ac.getBalance();
    return 0;
}
