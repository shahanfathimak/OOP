// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class electronics{
  public:
  string item;
  string brand;
  electronics(){
      item="television";
       brand="sony";
  }
  void print(){
      cout<<"item:"<<item<<endl;
       cout<<"brand:"<<brand<<endl;
  }
  
};


int main() {
 electronics o;
 o.print();
    return 0;
}
